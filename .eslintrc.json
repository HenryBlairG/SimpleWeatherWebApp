{
  "extends": "google",
  "parserOptions": {
    "ecmaVersion": "2015",
    "sourceType": "module"
  },
  "rules": {
    "comma-dangle": ["error", "never"],
    "no-undef": "error",
    "new-cap": ["error", { "capIsNewExceptions": ["DataTable"] }],
    "quote-props": ["error", "as-needed"],
    "space-in-parens": ["error", "never"],
    "indent": ["error", 2]
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "globals": {
  }
}
